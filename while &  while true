#while loop 1

count=1
while count<=100:
      print(count)
      count+=1
print("okyy")

#while loop 2

count=10
while count>=1:
    print(count)
    count-=1
print ("okkyy")

  #while true

count=1
while True:
    if count<=3:
        password=input("enter your password")
        if password=="nakki123":
            print("successfully logged in")
            break
        else:
            print("invalid password")
            count+=1
    else:
         print("oooopss limit reached")
         break
print("end")
        
